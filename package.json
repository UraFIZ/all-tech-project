{
  "name": "webpack-5",
  "version": "1.0.0",
  "private": true,
  "description": "typescript react lint prettier",
  "scripts": {
    "start": "set SERVE=true&& webpack serve",
    "watch": "webpack --watch",
    "build": "set NODE_ENV=production&& webpack",
    "build-dev": "webpack",
    "test:jest": "jest",
    "test:watch": "jest --verbose --watchAll --no-cache --runInBand",
    "tsc:check": "npm run tsc:src",
    "tsc:src": "tsc --noEmit --project tsconfig.json",
    "lint": "npm run prettier:fix && eslint \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier \"src/**/*.{ts,tsx}\""
  },
  "author": "Jimmy DC",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.13.6",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test && npm run tsc:check"
    }
  }
}
